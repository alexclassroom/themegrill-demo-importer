{
  "name": "themegrill-demo-importer",
  "title": "ThemeGrill Demo Importer",
  "version": "1.6.8",
  "homepage": "https://themegrill.com/demo-importer/",
  "repository": {
    "type": "git",
    "url": "https://github.com/themegrill/themegrill-demo-importer.git"
  },
  "license": "GPL-3.0+",
  "main": "Gruntfile.js",
  "scripts": {
    "build": "grunt && composer make-pot",
    "build-watch": "grunt watch",
    "lint:php": "composer lint:wpcs",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "git:update-hooks": "rm -r .git/hooks && node ./node_modules/husky/husky.js install"
  },
  "devDependencies": {
    "autoprefixer": "~9.4.10",
    "eslint": "~5.15.1",
    "grunt": "~1.0.3",
    "grunt-checktextdomain": "~1.0.1",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-contrib-cssmin": "~3.0.0",
    "grunt-contrib-jshint": "~2.0.0",
    "grunt-contrib-uglify": "~4.0.0",
    "grunt-contrib-watch": "~1.1.0",
    "grunt-phpcs": "~0.4.0",
    "grunt-postcss": "~0.9.0",
    "grunt-rtlcss": "~2.0.1",
    "grunt-sass": "~3.0.2",
    "grunt-stylelint": "~0.10.1",
    "grunt-wp-i18n": "~1.0.3",
    "node-sass": "~4.13.1",
    "stylelint": "~9.10.1",
    "stylelint-config-wordpress": "~13.1.0",
    "@wordpress/scripts": "^12.3.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0"
  },
  "engines": {
    "node": ">=8.9.3",
    "npm": ">=5.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.php": "npm run lint:php",
    "*.scss": "npm run lint:css",
    "*.js": "npm run lint:js"
  }
}
